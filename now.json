{
  "version": 2,
  "name": "www.thewdhanat.com",
  "scope": "thewdhanat-com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "^/(?!sw.js)(.+).(js|css)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "^/public/static/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "404.html",
      "status": 404
    }
  ]
}
